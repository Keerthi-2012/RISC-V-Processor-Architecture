#! /usr/bin/vvp
:ivl_version "11.0 (stable)";
:ivl_delay_selection "TYPICAL";
:vpi_time_precision + 0;
:vpi_module "/usr/lib/x86_64-linux-gnu/ivl/system.vpi";
:vpi_module "/usr/lib/x86_64-linux-gnu/ivl/vhdl_sys.vpi";
:vpi_module "/usr/lib/x86_64-linux-gnu/ivl/vhdl_textio.vpi";
:vpi_module "/usr/lib/x86_64-linux-gnu/ivl/v2005_math.vpi";
:vpi_module "/usr/lib/x86_64-linux-gnu/ivl/va_math.vpi";
S_0x59918273fb00 .scope module, "tb_shift_example" "tb_shift_example" 2 1;
 .timescale 0 0;
v0x599182752d80_0 .var "shift_amount", 5 0;
v0x599182752e70_0 .net "shifted_output", 63 0, v0x599182752c20_0;  1 drivers
v0x599182752f40_0 .var "test_input", 63 0;
S_0x59918273fc90 .scope module, "DUT" "shift_srl" 2 7, 3 2 0, S_0x59918273fb00;
 .timescale 0 0;
    .port_info 0 /INPUT 64 "a";
    .port_info 1 /INPUT 6 "n";
    .port_info 2 /OUTPUT 64 "result";
v0x5991827067b0_0 .net "a", 63 0, v0x599182752f40_0;  1 drivers
v0x599182706ba0_0 .var/i "i", 31 0;
v0x599182752b60_0 .net "n", 5 0, v0x599182752d80_0;  1 drivers
v0x599182752c20_0 .var "result", 63 0;
E_0x599182705870 .event edge, v0x599182752b60_0, v0x5991827067b0_0;
    .scope S_0x59918273fc90;
T_0 ;
    %wait E_0x599182705870;
    %pushi/vec4 0, 0, 64;
    %store/vec4 v0x599182752c20_0, 0, 64;
    %load/vec4 v0x599182752b60_0;
    %pad/u 32;
    %cmpi/u 64, 0, 32;
    %jmp/0xz  T_0.0, 5;
    %load/vec4 v0x599182752b60_0;
    %pad/u 32;
    %store/vec4 v0x599182706ba0_0, 0, 32;
T_0.2 ;
    %load/vec4 v0x599182706ba0_0;
    %cmpi/s 64, 0, 32;
    %jmp/0xz T_0.3, 5;
    %load/vec4 v0x5991827067b0_0;
    %load/vec4 v0x599182706ba0_0;
    %part/s 1;
    %load/vec4 v0x599182706ba0_0;
    %load/vec4 v0x599182752b60_0;
    %pad/u 32;
    %sub;
    %ix/vec4 4;
    %store/vec4 v0x599182752c20_0, 4, 1;
    %load/vec4 v0x599182706ba0_0;
    %addi 1, 0, 32;
    %store/vec4 v0x599182706ba0_0, 0, 32;
    %jmp T_0.2;
T_0.3 ;
T_0.0 ;
    %jmp T_0;
    .thread T_0, $push;
    .scope S_0x59918273fb00;
T_1 ;
    %pushi/vec4 3400182442, 0, 32;
    %concati/vec4 2863311530, 0, 32;
    %store/vec4 v0x599182752f40_0, 0, 64;
    %pushi/vec4 1, 0, 6;
    %store/vec4 v0x599182752d80_0, 0, 6;
    %delay 5, 0;
    %vpi_call 2 18 "$display", "Input: %b", v0x599182752f40_0 {0 0 0};
    %vpi_call 2 19 "$display", "Shifted by %d bits: %b", v0x599182752d80_0, v0x599182752e70_0 {0 0 0};
    %pushi/vec4 15, 0, 64;
    %store/vec4 v0x599182752f40_0, 0, 64;
    %pushi/vec4 4, 0, 6;
    %store/vec4 v0x599182752d80_0, 0, 6;
    %delay 5, 0;
    %vpi_call 2 25 "$display", "Input: %b", v0x599182752f40_0 {0 0 0};
    %vpi_call 2 26 "$display", "Shifted by %d bits: %b", v0x599182752d80_0, v0x599182752e70_0 {0 0 0};
    %vpi_call 2 29 "$finish" {0 0 0};
    %end;
    .thread T_1;
# The file index is used to find the file name in the following table.
